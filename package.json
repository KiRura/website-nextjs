{
	"name": "kirura-website-chakra-v3",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "bun run dep:gen && next build",
		"start": "next start",
		"check": "biome check --write --unsafe  . && tsc",
		"dep:gen": "license-report --only=prod --config license-report-config.json > src/app/license/licenses.json",
		"preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
		"deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
		"upload": "opennextjs-cloudflare build && opennextjs-cloudflare upload",
		"cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts"
	},
	"dependencies": {
		"@chakra-ui/react": "^3.22.0",
		"@emotion/react": "^11.14.0",
		"@opennextjs/cloudflare": "^1.5.3",
		"next": "15.3",
		"next-themes": "^0.4.6",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"react-icons": "^5.5.0",
		"swr": "^2.3.4"
	},
	"devDependencies": {
		"@biomejs/biome": "2.1.2",
		"@types/node": "^24.1.0",
		"@types/react": "^19.1.8",
		"@types/react-dom": "^19.1.6",
		"license-report": "^6.8.0",
		"typescript": "^5.8.3",
		"wrangler": "^4.25.1"
	},
	"trustedDependencies": [
		"@biomejs/biome",
		"@vercel/speed-insights",
		"esbuild",
		"sharp",
		"workerd"
	]
}
